{
  "name": "@gidw/event-emitter-js",
  "version": "0.2.1",
  "description": "JavaScript EventEmitter",
  "author": "Gilles De Waele",
  "license": "MIT",
  "main": "dist/index.min.js",
  "types": "src/index.d.ts",
  "keywords": [
    "javascript",
    "node",
    "browser",
    "events",
    "event",
    "eventemitter"
  ],
  "dependencies": {},
  "devDependencies": {
    "google-closure-compiler": "20200204.0.0",
    "mocha": "7.0.1",
    "standard": "14.3.1",
    "typescript": "3.8.2",
    "uglify-js": "3.8.0"
  },
  "repository": "github:GiDW/event-emitter-js.git",
  "bugs": {
    "url": "https://github.com/GiDW/event-emitter-js/issues",
    "email": "de.waele.gilles@gmail.com"
  },
  "scripts": {
    "lint": "standard src/**/*.{js,ts}",
    "build-typescript": "tsc",
    "build-uglify": "uglifyjs src/index.js --compress --mangle -o dist/index.uglify.js",
    "build": "google-closure-compiler --compilation_level ADVANCED --language_out ECMASCRIPT5_STRICT --formatting SINGLE_QUOTES --externs build/umd.extern.js --js_output_file dist/index.min.js src/index.js",
    "clean": "rm -rf dist/*",
    "test": "mocha test/test.js"
  },
  "standard": {
    "ignore": [
      "dist/**/*",
      "**/*.d.ts"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
}
