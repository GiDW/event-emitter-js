{
  "name": "@gidw/event-emitter-js",
  "version": "0.2.1",
  "description": "JavaScript EventEmitter",
  "author": "Gilles De Waele",
  "license": "MIT",
  "main": "dist/event_emitter.js",
  "browser": "dist/event_emitter.js",
  "types": "dist/event_emitter.d.ts",
  "keywords": [
    "javascript",
    "node",
    "browser",
    "events",
    "event",
    "eventemitter"
  ],
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-typescript": "4.1.1",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "google-closure-compiler": "20200406.0.0",
    "mocha": "7.1.1",
    "rollup": "2.7.2",
    "rollup-plugin-uglify": "6.0.4",
    "standard": "14.3.3",
    "typescript": "3.8.3",
    "uglify-js": "3.9.1"
  },
  "repository": "github:GiDW/event-emitter-js.git",
  "bugs": {
    "url": "https://github.com/GiDW/event-emitter-js/issues",
    "email": "de.waele.gilles@gmail.com"
  },
  "scripts": {
    "lint": "standard src/**/*.{js,ts}",
    "build-rollup": "rollup --config rollup.config.js",
    "build-uglify": "uglifyjs src/index.js --compress --mangle -o dist/index.uglify.js",
    "build-closure": "google-closure-compiler --compilation_level ADVANCED --language_out ECMASCRIPT5_STRICT --formatting SINGLE_QUOTES --externs build/umd.extern.js --js_output_file dist/index.min.js src/index.js",
    "clean": "rm -rf dist/*",
    "test": "mocha test/test.js"
  },
  "standard": {
    "ignore": [
      "dist/**/*",
      "**/*.d.ts"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
}
